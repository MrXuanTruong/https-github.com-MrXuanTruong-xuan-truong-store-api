        <script src="~/lib/vue-lazyload/vue-lazyload.js" asp-append-version="true"></script>

        <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
        <script src="~/lib/vue-loading-overlay/dist/vue-loading.min.js" asp-append-version="true"></script>
        <script src="~/js/shared/vue.prototype.min.js" asp-append-version="true"></script>
        <script src="~/js/shared/mixins/mixin.min.js" asp-append-version="true"></script>


        <script src="~/js/site.min.js" asp-append-version="true"></script>
        <script src="~/lib/vue/vue.min.js" asp-append-version="true"></script>
        <script src="~/lib/axios/axios.min.js" asp-append-version="true"></script>

        <script src="~/js/shared/localization.min.js" asp-append-version="true"></script>
